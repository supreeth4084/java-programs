import java.util.Scanner;

class Digit {
    static int sum=0, r, n; 
    public static void main(String[] args){
    Scanner sc = new Scanner(System.in);
    System.out.println("enter the value of n");
    int n = sc.nextInt();
    while(n>0){
        r = n % 10;
        n = n / 10;
        sum = sum + r;
    }
    System.out.println("sum of integer is " + sum);
    }
}
